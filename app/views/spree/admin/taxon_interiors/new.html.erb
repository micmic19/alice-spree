<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @taxon } %>

<% content_for :page_title do %>
  <%= Spree.t(:new_interior) %>
<% end %>
<%= form_for [:admin, @taxonomy, @taxon, @taxon_interior], html: { multipart: true } do |f| %>
  <fieldset data-hook="new_image">
    <%= render partial: 'form', locals: { f: f } %>

    <div class="form-actions" data-hook="buttons">
      <%= button Spree.t('actions.create'), 'save' %>
      <span class="or"><%= Spree.t(:or) %></span>
      <%= button_link_to Spree.t('actions.cancel'), spree.admin_taxonomy_taxon_taxon_interiors_path(@taxon), icon: 'delete', id: 'cancel_link' %>
    </div>
  </fieldset>
<% end %>
